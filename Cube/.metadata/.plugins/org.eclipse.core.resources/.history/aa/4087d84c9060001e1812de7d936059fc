#ifndef INC_GLOBAL_H_
#define INC_GLOBAL_H_

#include "main.h"
typedef struct{
	GPIO_TypeDef* port;
	GPIO_PinState pin;
} GPIO_config;

#define NUM_LEDS 12

GPIO_config led_hour[NUM_HOURS];

void set_GPIO_on(GPIO_config* destination);
void set_GPIO_off(GPIO_config* destination);
#endif /* INC_GLOBAL_H_ */
